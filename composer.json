{
    "name": "wsdl2phpgenerator/wsdl2phpgenerator",
    "description": "Simple class library for generating php classes from a wsdl file.",
    "keywords": ["soap", "wsdl"],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Wsdl2PhpGenerator\\": ["src/", "lib/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wsdl2PhpGenerator\\Tests\\": ["tests/src"]
        }
    },
    "require": {
        "php": ">=5.3.0",
        "ext-soap": "*",
        "symfony/options-resolver": "~2.3"
    },
    "require-dev": {
        "phing/phing": "~2.7",
        "php-vcr/phpunit-testlistener-vcr": "1.1.*",
        "phpunit/phpunit": "~4.0",
		"psr/log": "~1.0"
    }
}
